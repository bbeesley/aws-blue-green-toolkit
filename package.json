{
  "name": "aws-blue-green-toolkit",
  "version": "2.1.1",
  "description": "Utility functions to help with performing blue/green and canary deployments in AWS infrastructure",
  "author": "Bill Beesley <bill@beesley.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bbeesley/aws-blue-green-toolkit/issues"
  },
  "homepage": "https://github.com/bbeesley/aws-blue-green-toolkit#readme",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/main/index.js",
      "types": "./dist/main/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "rm -rf dist | true",
    "precompile": "npm run clean",
    "compile": "tsc",
    "dependabump": "ncu -u && rm -rf package-lock.json node_modules | true && npm i",
    "documentation": "./node_modules/.bin/documentation readme src/main/**.ts --github  --require-extension=ts --parse-extension=ts --babel=./babel.config.cjs --section=api",
    "lint": "eslint --ext '.ts' --ext '.cjs' --ext '.mjs' --ext '.js' .",
    "pretest": "npm run clean && run-p lint compile",
    "test": "ava",
    "prerelease": "npm run documentation && npm run compile",
    "release": "semantic-release --debug"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.5",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/aws-lambda": "^8.10.101",
    "@types/node": "^18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "ava": "^4.3.1",
    "aws-sdk-client-mock": "^2.0.0-beta.1",
    "conventional-changelog-angular": "^5.0.13",
    "conventional-commit-types": "^3.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "documentation": "^13.2.5",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbeesley/aws-blue-green-toolkit.git"
  },
  "keywords": [
    "aws",
    "codedeploy",
    "lambda",
    "cd",
    "ci",
    "deployments",
    "blue/green",
    "canary",
    "serverless",
    "rds",
    "aurora",
    "sqs",
    "sns",
    "ecs"
  ],
  "release": {
    "branches": [
      "master",
      "beta",
      "alpha"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "chore",
              "scope": "deps*",
              "release": "patch",
              "subject": "[security]*"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/github",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md",
            "README.md"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@aws-sdk/client-application-auto-scaling": "^3.145.0",
    "@aws-sdk/client-cloudwatch": "^3.146.0",
    "@aws-sdk/client-cloudwatch-events": "^3.145.0",
    "@aws-sdk/client-dynamodb": "^3.145.0",
    "@aws-sdk/client-kinesis": "^3.145.0",
    "@aws-sdk/client-lambda": "^3.148.0",
    "@aws-sdk/client-rds": "^3.145.0",
    "@aws-sdk/client-sns": "^3.145.0",
    "@aws-sdk/client-sqs": "^3.145.0"
  },
  "ava": {
    "files": [
      "src/test/*.test.js"
    ],
    "failFast": false,
    "failWithoutAssertions": false,
    "verbose": true,
    "concurrency": 1,
    "timeout": "1m"
  }
}
